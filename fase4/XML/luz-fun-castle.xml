<scene>
	<lights>
        <light type="POINT" posX="0" posY="2" posZ="0"/>
    </lights>

	<group>
		<scale X="10" Y="2"/>

		<group>
			<rotate angle="90" axisX="0" axisY="1" axisZ="0"/>

			<group>
				<!--Parede direita-->
				<translate X="5.5"/>
				<models>
					<model file="box.3d" diffR="0.5" diffG="0.5" diffB="0.5" specR="0.5" specG="0.5" specB="0.5" ambR="0.4" ambG="0.4" ambB="0.4"/>
				</models>
			</group>

			<group>
				<!--Parede esquerda-->
				<translate X="-5.5"/>
				<models>
					<model file="box.3d" diffR="0.5" diffG="0.5" diffB="0.5" specR="0.5" specG="0.5" specB="0.5" ambR="0.4" ambG="0.4" ambB="0.4"/>
				</models>
			</group>
		</group>

		<group>
			<!--Parede trás-->
			<translate Z="-5.5"/>
			<models>
				<model file="box.3d" diffR="0.5" diffG="0.5" diffB="0.5" specR="0.5" specG="0.5" specB="0.5" ambR="0.4" ambG="0.4" ambB="0.4"/>
			</models>
		</group>
	</group>

	<group>
		<!--Parede frente-->
        <translate Z="5.5"/>
        <models>
            <model file="box4.3d" diffR="0.5" diffG="0.5" diffB="0.5" specR="0.5" specG="0.5" specB="0.5" ambR="0.4" ambG="0.4" ambB="0.4"/>
        </models>
    </group>

	<group>
		<scale Y="3"/>
		<translate Y="0.5"/>

		<group>
			<!--Torre frente direita-->
			<rotate time="1" axisY="1"/>
			<translate time="4">
				<point X="5.5" Z="5.5"/>
				<point X="5.5" Z="-5.5"/>
				<point X="-5.5" Z="-5.5"/>
				<point X="-5.5" Z="5.5"/>
            </translate>
			<models>
				<model file="box.3d" diffR="0.2" diffG="0.2" diffB="0.8" specR="0.2" specG="0.2" specB="0.8" ambR="0.2" ambG="0.2" ambB="0.8"/>
			</models>

			<group>
				<!--Telhado frente direita-->
				<translate time="1">
					<point Y="1.5"/>
					<point Y="2"/>
					<point Y="3"/>
					<point Y="2"/>
				</translate>
				<models>
					<model file="cone.3d" diffR="0.2" diffG="0.8" diffB="0.2" specR="0.2" specG="0.8" specB="0.2" ambR="0.2" ambG="0.8" ambB="0.2"/>
				</models>
			</group>
		</group>

		<group>
			<!--Torre frente esquerda-->
			<rotate time="1" axisY="1"/>
			<translate time="4">
				<point X="5.5" Z="-5.5"/>
				<point X="-5.5" Z="-5.5"/>
				<point X="-5.5" Z="5.5"/>
				<point X="5.5" Z="5.5"/>
            </translate>
			<models>
				<model file="box.3d" diffR="0.2" diffG="0.2" diffB="0.8" specR="0.2" specG="0.2" specB="0.8" ambR="0.2" ambG="0.2" ambB="0.8"/>
			</models>

			<group>
				<!--Telhado frente esquerda-->
				<translate time="1">
					<point Y="1.5"/>
					<point Y="2"/>
					<point Y="3"/>
					<point Y="2"/>
				</translate>
				<models>
					<model file="cone.3d" diffR="0.2" diffG="0.8" diffB="0.2" specR="0.2" specG="0.8" specB="0.2" ambR="0.2" ambG="0.8" ambB="0.2"/>
				</models>
			</group>
		</group>

		<group>
			<!--Torre trás direita-->
			<rotate time="1" axisY="1"/>
			<translate time="4">
				<point X="-5.5" Z="-5.5"/>
				<point X="-5.5" Z="5.5"/>
				<point X="5.5" Z="5.5"/>
				<point X="5.5" Z="-5.5"/>
            </translate>
			<models>
				<model file="box.3d" diffR="0.2" diffG="0.2" diffB="0.8" specR="0.2" specG="0.2" specB="0.8" ambR="0.2" ambG="0.2" ambB="0.8"/>
			</models>

			<group>
				<!--Telhado trás direita-->
				<translate time="1">
					<point Y="1.5"/>
					<point Y="2"/>
					<point Y="3"/>
					<point Y="2"/>
				</translate>
				<models>
					<model file="cone.3d" diffR="0.2" diffG="0.8" diffB="0.2" specR="0.2" specG="0.8" specB="0.2" ambR="0.2" ambG="0.8" ambB="0.2"/>
				</models>
			</group>
		</group>

		<group>
			<!--Torre trás esquerda-->
			<rotate time="1" axisY="1"/>
			<translate time="4">
				<point X="-5.5" Z="5.5"/>
				<point X="5.5" Z="5.5"/>
				<point X="5.5" Z="-5.5"/>
				<point X="-5.5" Z="-5.5"/>
            </translate>
			<models>
				<model file="box.3d" diffR="0.2" diffG="0.2" diffB="0.8" specR="0.2" specG="0.2" specB="0.8" ambR="0.2" ambG="0.2" ambB="0.8"/>
			</models>

			<group>
				<!--Telhado trás esquerda-->
				<translate time="1">
					<point Y="1.5"/>
					<point Y="2"/>
					<point Y="3"/>
					<point Y="2"/>
				</translate>
				<models>
					<model file="cone.3d" diffR="0.2" diffG="0.8" diffB="0.2" specR="0.2" specG="0.8" specB="0.2" ambR="0.2" ambG="0.8" ambB="0.2"/>
				</models>
			</group>
		</group>
	</group>

	<group>
		<scale Y="2"/>

		<group>
			<!--Telhado centro direita-->
			<rotate time="1" axisY="1"/>
			<translate time="2">
				<point X="2" Y="1" Z="5.5"/>
				<point X="0" Y="2" Z="5.5"/>
				<point X="-2" Y="1" Z="5.5"/>
				<point X="0" Y="2" Z="5.5"/>
            </translate>
			<models>
				<model file="cone.3d" diffR="0.2" diffG="0.8" diffB="0.2" specR="0.2" specG="0.8" specB="0.2" ambR="0.2" ambG="0.8" ambB="0.2"/>
			</models>
		</group>

		<group>
			<!--Telhado centro esquerda-->
			<rotate time="1" axisY="1"/>
			<translate time="2">
				<point X="-2" Y="1" Z="5.5"/>
				<point X="0" Y="2" Z="5.5"/>
				<point X="2" Y="1" Z="5.5"/>
				<point X="0" Y="2" Z="5.5"/>
            </translate>
			<models>
				<model file="cone.3d" diffR="0.2" diffG="0.8" diffB="0.2" specR="0.2" specG="0.8" specB="0.2" ambR="0.2" ambG="0.8" ambB="0.2"/>
			</models>
		</group>
	</group>

    <group>
		<!--Lua-->
		<rotate time="1" axisY="1"/>
		<translate time="2">
				<point Y="7" Z="-30" X="-2.5"/>
				<point Y="7" Z="-20" X="-5"/>
				<point Y="7" Z="-10" X="-2.5"/>
				<point Y="7" Z="-20" X="0"/>
            </translate>
        <translate Y="7" Z="-30" X="-2.5"/>
        <models>
            <model file="sphere.3d" texture="moon.jpg"/>
        </models>
    </group>
</scene>
