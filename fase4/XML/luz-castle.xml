			</models>

			<group>
				<!--Telhado frente direita-->
				<translate Y="1.5"/>
				<models>
					<model file="cone.3d" texture="button.jfif" diffR="0.5" diffG="0.5" diffB="0.5" specR="0.5" specG="0.5" specB="0.5" ambR="0.4" ambG="0.4" ambB="0.4"/>
