    <group>
		<!--Lua-->
        <translate Y="7" Z="-30" X="-2.5"/>
        <models>
            <model file="sphere.3d" texture="moon.jpg" diffR="0.5" diffG="0.5" diffB="0.5" specR="0.5" specG="0.5" specB="0.5" emisX="0.5" emisY="0.5" emisZ="0.5" ambR="0.4" ambG="0.4" ambB="0.4"/>
