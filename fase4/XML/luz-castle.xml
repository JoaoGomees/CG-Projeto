	<group>
		<scale X="10" Y="2"/>

		<group>
			<rotate angle="90" axisX="0" axisY="1" axisZ="0"/>

			<group>
				<!--Parede direita-->
				<translate X="5.5"/>
				<models>
					<model file="box.3d" texture="button.jfif" diffR="0.5" diffG="0.5" diffB="0.5" specR="0.5" specG="0.5" specB="0.5" />
				</models>
			</group>

			<group>
				<!--Parede esquerda-->
				<translate X="-5.5"/>
				<models>
					<model file="box.3d" texture="button.jfif" diffR="0.5" diffG="0.5" diffB="0.5" specR="0.5" specG="0.5" specB="0.5" />
				</models>
			</group>
		</group>

		<group>
			<!--Parede trás-->
			<translate Z="-5.5"/>
			<models>
				<model file="box.3d" texture="button.jfif" diffR="0.5" diffG="0.5" diffB="0.5" specR="0.5" specG="0.5" specB="0.5" />
			</models>
		</group>
	</group>

	<group>
		<!--Parede frente-->
        <translate Z="5.5"/>
